{
  "name": "prisma-with-admin",
  "version": "0.0.2",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "generate": "yarn generate:prisma && yarn generate:crud && yarn generate:nexus && yarn generate:admin && yarn generate:graphql",
    "generate:prisma": "prisma generate",
    "generate:crud": "ts-node --transpile-only -P nexus.tsconfig.json scripts/createCRUD",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/Api/schema",
    "generate:admin": "ts-node --transpile-only -P nexus.tsconfig.json scripts/generateAdmin",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "db-start": "docker-compose up -d",
    "db-stop": "docker-compose down",
    "create-migration": "prisma migrate save --experimental",
    "migrate-database": "prisma migrate up --experimental --auto-approve --create-db"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-rc.4",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@nexus/schema": "0.14.0",
    "@prisma-tools/admin": "1.0.10",
    "@prisma-tools/delete": "0.0.14",
    "@prisma-tools/nexus": "0.6.24",
    "@prisma/client": "2.0.0",
    "apollo-server-micro": "2.14.4",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "graphql-tools": "6.0.9",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "next": "latest",
    "nexus-schema-plugin-prisma-select": "0.0.4",
    "oah-eva-icon": "1.0.1",
    "oah-ui": "2.1.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.0.1",
    "ts-node": "8.10.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.3",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-operations": "1.15.3",
    "@graphql-codegen/typescript-react-apollo": "1.15.3",
    "@prisma/cli": "2.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.12.47",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-select": "3.0.13",
    "@types/styled-components": "4.4.3",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-plugin-graphql-tag": "2.5.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "typescript": "3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
