{
  "name": "prisma-admin",
  "version": "0.0.2",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/Api/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "create-migration": "prisma migrate save --experimental",
    "migrate-database": "prisma migrate up --experimental --auto-approve --create-db"
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "@apollo/react-ssr": "4.0.0",
    "@nexus/schema": "0.15.0",
    "@paljs/nexus": "1.1.9",
    "@paljs/admin": "1.2.0",
    "@paljs/ui": "1.0.3",
    "@paljs/icons": "1.0.1",
    "@prisma/client": "2.5.1",
    "apollo-server-micro": "2.16.1",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "graphql": "14.7.0",
    "graphql-tag": "2.11.0",
    "graphql-tools": "6.0.18",
    "jsonwebtoken": "8.5.1",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1",
    "ts-node": "8.10.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/typescript": "1.17.9",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "1.17.8-alpha-f207402d.0",
    "@prisma/cli": "2.5.1",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.6.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "4.4.3",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "babel-plugin-graphql-tag": "2.5.0",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.2",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "repository": "https://github.com/AhmedElywa/prisma-admin",
  "description": "Full stack Prisma, GraphQL, Nexus, Apollo and NextJS with Admin UI to control your Prisma db"
}
