{
  "name": "prisma-with-admin",
  "version": "0.0.2",
  "author": "Ahmed Elywa",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "yarn generate && next build",
    "start": "next start",
    "generate": "yarn generate:prisma && yarn generate:crud && yarn generate:nexus && yarn generate:admin && yarn generate:graphql",
    "generate:prisma": "prisma generate",
    "generate:crud": "ts-node --transpile-only -P nexus.tsconfig.json scripts/createCRUD",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/Api/schema",
    "generate:admin": "ts-node --transpile-only -P nexus.tsconfig.json scripts/generateadmin",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "create-migration": "prisma migrate save --experimental",
    "migrate-database": "prisma migrate up --experimental --auto-approve --create-db"
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.50",
    "@apollo/react-ssr": "4.0.0-beta.1",
    "@nexus/schema": "0.13.1",
    "@prisma-tools/admin": "0.0.20",
    "@prisma-tools/delete": "0.0.7",
    "@prisma-tools/nexus": "0.6.12",
    "@prisma/client": "2.0.0-beta.6",
    "apollo-server-micro": "2.9.16",
    "apollo-utilities": "1.3.4",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.0",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.1",
    "jsonwebtoken": "8.5.1",
    "lowdb": "1.0.0",
    "moment": "2.26.0",
    "next": "latest",
    "nexus-schema-plugin-prisma-select": "0.0.2",
    "oah-eva-icon": "1.0.1",
    "oah-ui": "2.1.4",
    "react": "16.13.1",
    "react-beautiful-dnd": "13.0.0",
    "react-datepicker": "2.16.0",
    "react-dom": "16.13.1",
    "react-hook-form": "5.7.2",
    "react-quill": "2.0.0-beta1",
    "react-table": "7.1.0",
    "styled-components": "5.1.1",
    "ts-node": "8.10.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.15.0",
    "@graphql-codegen/typescript": "1.15.0",
    "@graphql-codegen/typescript-operations": "1.15.0",
    "@graphql-codegen/typescript-react-apollo": "1.15.0",
    "@prisma/cli": "2.0.0-beta.6",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lowdb": "1.0.9",
    "@types/node": "12.12.42",
    "@types/react": "16.9.35",
    "@types/react-beautiful-dnd": "12.1.3",
    "@types/react-datepicker": "2.11.0",
    "@types/react-dom": "16.9.8",
    "@types/react-select": "3.0.13",
    "@types/react-table": "7.0.18",
    "@types/styled-components": "4.4.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-plugin-graphql-tag": "2.5.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "jest-dom": "4.0.0",
    "lerna": "3.22.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "typescript": "3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  }
}
